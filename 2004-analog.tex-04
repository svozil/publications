TensorProductVec[a_, b_] :=
Flatten[
Table[(*a, b are nx1 and mx1 - matrices*)
     a[[s]]*b[[t]],
    {s, 1,Length[a]},
  {t, 1, Length[b]}]
];

TensorProduct3Vec[a_, b_, c_] :=
Flatten[
Table[(*a, b are nx1 and mx1 - matrices*)
     a[[s]]*b[[t]]*c[[u]],
    {s, 1,Length[a]},
  {t, 1, Length[b]},
{u, 1, Length[c]}
     ]
];


