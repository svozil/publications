

(*


NASA MASIE  Data Set ID: G02186 2006-present

https://nsidc.org/data/G02186/versions/1


*)


IceExt= ReadList["\\mytex\\2019-MASIE-MASIE-2006-2019.txt",
   Number, RecordLists -> True];


DataIceExt = Table[{i,IceExt[[i,1]]},{i,1,Length[IceExt]}];

DataModelIceExt = LinearModelFit[DataIceExt,x,x];
N[Normal[DataModelIceExt] ]

DataModelIceExt["ParameterTable"]


Show[{
ListPlot[DataIceExt,
 PlotRange -> {{-100,Length[IceExt]+100},{-500000,17500000}},
 Axes -> False,
 Frame -> True,
 FrameStyle -> Directive[Blue, 30, FontFamily -> "Times"],
 FrameTicks -> {
   {{
    {0,0},
    {2500000,2.5},
    {5000000,5},
    {7500000,7.5},
    {10000000,10},
    {12500000,12.5},
    {15000000,15},
    {17500000,17.5}
    }, None},
   {{
    {1, "2006-01"},
    {1424, "2010-01"},
    {2848, "2014-01"},
    {4272, "2018-01"},
    {5060, "2019-12"}
    }, None}
               },
 FrameTicksStyle -> Black,
 FrameLabel -> {"year", "Arctic Ice Extent (million square km)"},
 Joined -> True,
 PlotStyle -> Directive[Blue],
 Filling -> Axis
],
ListPlot[MovingAverage[DataIceExt,365],
 PlotRange -> All,
 PlotStyle -> {Green,PointSize[0.005]},
 Frame -> False,
 Axes -> False
],
Plot[DataModelIceExt[x],{x,1,Length[IceExt]},
      Frame -> False,
      Axes->False,PlotStyle -> {Orange, Dashed, Thickness[0.01]}
]
},ImageSize -> 1000]

