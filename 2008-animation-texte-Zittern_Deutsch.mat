PendelFreqY = 104;

Xmpos[t_] := 0 (*Sin[t/PendelFreqX]*(TextWidthVary/1.2)*);
Ympos[t_] := 0 (*Sin[t/PendelFreqY]*(TextHeightVary/1.2)*);


