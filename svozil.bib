@Article{Ardavan-89,
  Title                    = {The Speed-of-Light Catastrophe},
  Author                   = {Houshang Ardavan},
  Journal                  = {Proceedings of the Royal Society of London. A. Mathematical and Physical Sciences},
  Year                     = {1989},
  Number                   = {1866},
  Pages                    = {113-141},
  Volume                   = {424},

  Abstract                 = {It is shown that the retarded potential for an extended source, whose density depends on the azimuthal angle $\varphi$ and the time t in the combination $\varphi$-$\omega$t only, has a discontinuous gradient at points within the source that lie on the cylindrical surface r$_P$ = c/$\omega$, and that the radial component of this gradient diverges like (r$_P$-c/$\omega$)${-^\frac{1}{4}}$ as r$_P\omega$/c $\rightarrow$ 1+, where r$_P$ is the radial position of the observation point in cylindrical polar coordinates, c is the propagation speed of the waves described by the retarded potential, and $\omega$ is a constant angular frequency. So, for an extended source moving on a curved path whose speed is in parts smaller and in parts greater than the speed of the disturbances it generates, there is a sudden and infinitely large change in the strength of the field across the surface (within the source) at which the speed of the source and the speed of the waves are equal. The divergence in the field strength does not arise from any long-term transfer of energy to the field; it occurs instantaneously and so is to be understood either as signifying the breakdown of the governing wave equation or as disallowing the type of source in question. Some implications of this non-elementary catastrophe - in the contexts of gas-dynamics, electrodynamics and general relativity - are also pointed out.},
  Doi                      = {10.1098/rspa.1989.0072},
  Url                      = {https://doi.org/10.1098/rspa.1989.0072}
}

